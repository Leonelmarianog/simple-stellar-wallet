  "private": true,
  "dependencies": {
    "@emotion/react": "^11.7.0",
    "@emotion/styled": "^11.6.0",
    "@mui/icons-material": "^5.2.0",
    "@mui/material": "^5.2.2",
    "@testing-library/jest-dom": "^5.15.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "@types/jest": "^26.0.24",
    "@types/node": "^12.20.37",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "@types/react-router-dom": "^5.3.2",
    "@types/sjcl": "^1.0.30",
    "cypress": "^9.1.0",
    "emotion-normalize": "^11.0.1",
    "eslint-config-prettier": "^8.3.0",
    "formik": "^2.2.9",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.2",
    "prettier": "^2.5.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-normalize": "^0.4.4",
    "react-router-dom": "^6.0.2",
    "react-scripts": "4.0.3",
    "sjcl": "^1.0.8",
    "stellar-sdk": "^9.1.0",
    "typescript": "^4.5.2",
    "web-vitals": "^1.1.2",
    "yup": "^0.32.11"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "test:ui:dev": "./node_modules/.bin/cypress open",
    "test:ui:headless": "./node_modules/.bin/cypress run",
    "prettier:check": "./node_modules/.bin/prettier --check \"src/**/*.{js,jsx,ts,tsx,json}\"",
    "prettier:fix": "./node_modules/.bin/prettier --write \"src/**/*.{js,jsx,ts,tsx,json}\""
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "prettier"
    ]
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
